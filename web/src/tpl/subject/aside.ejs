        <nav class="btn-toolbar">
          <div class="btn-group">
            <span class="btn btn-primary active">资源 <%-subjectResourceCount%></span>
            <span class="btn btn-primary">参与人 <%-memberCount%></span>
            <span class="btn btn-primary">统计</span>
          </div>
        </nav>

        <div class="artice-aside-img">
          <!--
          <div class="video">
            <img width="100%" height="200" src="http://imgsrc.baidu.com/forum/w%3D580/sign=3b95cec70c3387449cc52f74610ed937/f074d0fc1e178a8274b0ef37f603738da877e868.jpg" />
            预览  标注 下载  删除
          </div>
          -->
          <div class="img-list">
            <%
              for(var i in resourceList){
              var item = resourceList[i];
            %>
            <div class="sub-resource-<%-item.id%>">
            <%if(item.type === 1){%>
              <img width="100%" height="100" src="/cgi/resource/preview?id=<%-item.id%>" title="<%-item.name%>"  data-id="<%-item.id%>"  data-action="review" />
              <a data-id="<%-item.id%>"  data-action="review">预览</a>  <a href="/cgi/resource/download?id=<%-item.id%>" target="_blank">下载</a>  
              <%if(my.auth || my.id === creator){ %>
                <a data-action="deleteResource"  data-id="<%-item.id%>">删除</a>
              <%}%>
            <%}else if(item.type === 4 || item.type ===3){%>
              <video src="/cgi/resource/preview?id=<%-item.id%>" controls="controls">
              您的浏览器不支持 video 标签。
              </video>
              <a data-id="<%-item.id%>"  data-action="mark">标注</a>  <a data-id="<%-item.id%>"  data-action="review">预览</a>  <a href="/cgi/resource/download?id=<%-item.id%>" target="_blank">下载</a>
              <%if(my.auth || my.id === creator){ %>
                <a data-action="deleteResource"  data-id="<%-item.id%>">删除</a>
              <%}%>
            <%}else{%>
              <p><%-item.name%></p>
              <a href="/cgi/resource/download?id=<%-item.id%>" target="_blank">下载</a>
              <%if(my.auth || my.id === creator){ %>
                <a data-action="deleteResource"  data-id="<%-item.id%>">删除</a>
              <%}%>              
            <%}%>
            </div>
            <%}%>
          </div>
        </div>